<?xml version="1.0" encoding="UTF-8"?>
<model name="inventory" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xmlns="http://schema.qcadoo.org/model"
	xsi:schemaLocation="http://schema.qcadoo.org/model http://schema.qcadoo.org/model.xsd">
	<fields>
		<string name="number" required="true" unique="true">
				<validatesLength max="40" />
		</string>
		<belongsTo name="warehouse" model="warehouse" plugin="inventory"/>
		<belongsTo name="product" model="product" plugin="basic" />
		<decimal name="quantity" />
	</fields>
	<hooks>
	</hooks>	
</model>