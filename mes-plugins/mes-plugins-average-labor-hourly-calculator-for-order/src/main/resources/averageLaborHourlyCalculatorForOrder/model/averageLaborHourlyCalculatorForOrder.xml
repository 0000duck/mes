<?xml version="1.0" encoding="UTF-8"?>
<model name="averageLaborHourlyCalculatorForOrder" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xmlns="http://schema.qcadoo.org/model"
	xsi:schemaLocation="http://schema.qcadoo.org/model http://schema.qcadoo.org/model.xsd">
	<fields>
		<datetime name="startDate"/>
		<datetime name="finishDate"/>
		<belongsTo name="order" model="order" plugin="orders" />
		<belongsTo name="productionLine" model="productionLine" plugin="productionLines"/>
		<belongsTo name="assignmentToShift" model="assignmentToShift" />
		<hasMany name="assignmentToShifts" joinField="assignmentToShift" model="assignmentToShift" plugin="assignmentToShift"/>
		<enum values="01assignment" name="basedOn"/>
		<decimal name="averageLaborHourlyCost" />
	</fields>
	<hooks/>
	<identifier expression="#number + ' - ' + #name" />
</model>