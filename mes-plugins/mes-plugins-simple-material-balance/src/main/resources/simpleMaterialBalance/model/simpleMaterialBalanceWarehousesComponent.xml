<?xml version="1.0" encoding="UTF-8"?>
<model name="simpleMaterialBalanceWarehousesComponent" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xmlns="http://schema.qcadoo.org/model"
	xsi:schemaLocation="http://schema.qcadoo.org/model http://schema.qcadoo.org/model.xsd">
	<fields>
		<belongsTo name="simpleMaterialBalance" model="simpleMaterialBalance"
			lazy="false" required="true" />
		<belongsTo name="warehouse" plugin="inventory" model="warehouse" lazy="false" required="true" />
	</fields>
	<hooks>
	</hooks>
	<identifier expression="#warehouse['number'] + ' - ' + #warehouse['name']" />
</model>	