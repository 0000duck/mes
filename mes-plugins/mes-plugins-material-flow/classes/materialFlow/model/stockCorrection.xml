<?xml version="1.0" encoding="UTF-8"?>
<model name="stockCorrection" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xmlns="http://schema.qcadoo.org/model"
	xsi:schemaLocation="http://schema.qcadoo.org/model http://schema.qcadoo.org/model.xsd">
	<fields>
		<string name="number" required="true" unique="true">
				<validatesLength max="40" />
		</string>
		<datetime name="date" readonly="true" />
		<belongsTo name="stockAreas" model="stockAreas" />
		<belongsTo name="product" model="product" plugin="basic"/>
		<decimal name="shouldBe" persistent="false" required="true">
		</decimal>
		<decimal name="found" required="true">
		</decimal>
		<belongsTo name="staff" model="staff" plugin="basic" />
	
	</fields>
	<hooks>
	</hooks>	
</model>