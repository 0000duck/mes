<?xml version="1.0" encoding="UTF-8"?>
<model name="conversionItem" activable="true"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns="http://schema.qcadoo.org/model"
	xsi:schemaLocation="http://schema.qcadoo.org/model http://schema.qcadoo.org/model.xsd">
	<fields>

		<string name="quantityFrom" required="true" />
		<string name="quantityTo" required="true" />
		<dictionary name="unitFrom" dictionary="units" />
		<dictionary name="unitTo" dictionary="units" />
		<belongsTo name="conversion" model="conversion" />
		<belongsTo name="product" model="product" />
		
	</fields>

	<hooks>
	</hooks>
</model>	