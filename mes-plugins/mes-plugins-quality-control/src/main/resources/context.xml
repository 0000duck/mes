<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xmlns:context="http://www.springframework.org/schema/context"
	xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans-3.0.xsd http://www.springframework.org/schema/context http://www.springframework.org/schema/context/spring-context-3.0.xsd">

	<context:component-scan base-package="com.qcadoo.mes.qualityControl" />
	
	<bean id="qualityControlForOrderPdfView"
  		class="com.qcadoo.mes.qualityControl.print.QualityControlForOrderPdfView"/>
  		
  	<bean id="qualityControlForUnitPdfView"
  		class="com.qcadoo.mes.qualityControl.print.QualityControlForUnitPdfView"/>
  		
  	<bean id="qualityControlForBatchPdfView"
  		class="com.qcadoo.mes.qualityControl.print.QualityControlForBatchPdfView"/>
  		
  	<bean id="qualityControlForOperationPdfView"
  		class="com.qcadoo.mes.qualityControl.print.QualityControlForOperationPdfView"/>

	<bean id="qualityControlForOrderXlsView"
  		class="com.qcadoo.mes.qualityControl.print.QualityControlForOrderXlsView"/>
  		
  	<bean id="qualityControlForUnitXlsView"
  		class="com.qcadoo.mes.qualityControl.print.QualityControlForUnitXlsView"/>
  		
  	<bean id="qualityControlForBatchXlsView"
  		class="com.qcadoo.mes.qualityControl.print.QualityControlForBatchXlsView"/>
  		
  	<bean id="qualityControlForOperationXlsView"
  		class="com.qcadoo.mes.qualityControl.print.QualityControlForOperationXlsView"/>

</beans>